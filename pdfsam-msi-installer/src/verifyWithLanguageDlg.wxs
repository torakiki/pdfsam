          				<Publish Property="LOCALE_CODE" Value="nl">LOCALE = "Dutch"</Publish>
          				<Publish Property="LOCALE_CODE" Value="en_GB">LOCALE = "English (UK)"</Publish>
          				<Publish Property="LOCALE_CODE" Value="fi">LOCALE = "Finnish"</Publish>
